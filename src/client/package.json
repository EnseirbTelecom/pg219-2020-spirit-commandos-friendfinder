{
  "name": "friend-finder",
  "private": true,
  "version": "1.0.0",
  "description": "Friend Finder",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run serve",
    "serve": "http-server ./www/ -o -c 1 -a localhost -p 8082",
    "build-cordova": "node ./build/build.js && cd cordova && cordova build",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./www/fonts/"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "dom7": "^2.1.3",
    "framework7": "^5.6.0",
    "framework7-icons": "^3.0.0",
    "template7": "^1.4.2"
  },
  "devDependencies": {
    "cpy": "^8.1.0",
    "cpy-cli": "^3.1.0",
    "http-server": "^0.12.1",
    "rimraf": "^3.0.2"
  }
}
